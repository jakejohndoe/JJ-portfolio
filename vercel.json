{
    "version": 2,
    "builds": [
      {
        "src": "client/vite.config.ts",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/dist",
          "buildCommand": "npm run build:frontend"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }