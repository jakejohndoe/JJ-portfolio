{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "https://hellojakejohn.onrender.com/api/health"
    },
    {
      "src": "/api/stats",
      "dest": "https://hellojakejohn.onrender.com/api/stats"
    },
    {
      "src": "/api/projects",
      "dest": "https://hellojakejohn.onrender.com/api/projects"
    },
    {
      "src": "/api/skills",
      "dest": "https://hellojakejohn.onrender.com/api/skills"
    },
    {
      "src": "/assets/(.*)",
      "dest": "client/dist/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "client/dist/index.html"
    }
  ]
}